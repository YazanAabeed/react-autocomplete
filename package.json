{
  "name": "@yazanaabed/react-autocomplete",
  "version": "0.1.2",
  "description": "This autocomplete component built to show what render props pattern can do with react and how much it flexible",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "src": "src"
  },
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compile": "npm run compile-dev && npm run compile-prod",
    "compile-dev": "NODE_ENV=development babel src --out-dir lib/dev --copy-files",
    "compile-prod": "NODE_ENV=production babel lib/dev --out-dir lib/prod --copy-files"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YazanAabeed/react-autocomplete.git"
  },
  "author": "yazanaabed <yazanaabed2@gmail.com> (https://medium.com/@yazanaabed)",
  "license": "MIT",
  "keywords": [
    "react",
    "reactjs",
    "autocomplete",
    "components",
    "state-management"
  ],
  "bugs": {
    "url": "https://github.com/YazanAabeed/react-autocomplete/issues"
  },
  "homepage": "https://github.com/YazanAabeed/react-autocomplete#readme",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-preset-minify": "^0.5.0"
  }
}
